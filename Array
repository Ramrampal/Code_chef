#include <bits/stdc++.h>
using namespace std;


int main() {
    int n, X;
    cin >> n >> X;
    int arr[n];
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }
    for (int i = 0; i < n; i++) {
        if (arr[i] == X) {
            cout << "YES";
            return 0;
        }
    }
    cout << "NO";
    return 0;
}





Sample Input
5 3
7 3 5 2 1
Your Output
YES
