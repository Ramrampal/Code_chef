#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T;  // Read the number of test cases

    // Loop for each test case
    for (int i = 0; i < T; i++) {
        int X;
        cin >> X;  // Read the distance of the office

        // Chef travels to office and back, so the total distance per day is 2 * X
        // In a week (5 working days), Chef will travel 5 * (2 * X)
        int totalDistance = 2 * X * 5;

        // Output the result for this test case
        cout << totalDistance << endl;
    }

    return 0;
}
